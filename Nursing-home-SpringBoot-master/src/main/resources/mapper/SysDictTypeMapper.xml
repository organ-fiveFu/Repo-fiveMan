<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nurse.healthy.mapper.SysDictTypeMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.nurse.healthy.model.entity.sys.SysDictType" id="sysDictTypeMap">
        <result property="id" column="id"/>
        <result property="dictTypeCode" column="dict_type_code"/>
        <result property="dictTypeName" column="dict_type_name"/>
        <result property="wubiCode" column="wubi_code"/>
        <result property="pinyinCode" column="pinyin_code"/>
        <result property="sortNo" column="sort_no"/>
        <result property="remark" column="remark"/>
        <result property="creatorId" column="creator_id"/>
        <result property="updaterId" column="updater_id"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="isDel" column="is_del"/>
        <result property="useFlag" column="use_flag"/>
    </resultMap>
    <update id="delByTypeCode">
        update sys_dict_type
        set is_del = 1
        where dict_type_code = #{TypeCode}
    </update>


    <select id="selectAllCode" resultMap="sysDictTypeMap">
        select * from sys_dict_type where is_del = 0
    </select>

</mapper>